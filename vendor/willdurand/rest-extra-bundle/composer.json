{
    "name": "willdurand/rest-extra-bundle",
    "description": "This bundle provides extra features for your REST APIs built using Symfony2.",
    "keywords": [ "rest", "api" ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
        "name": "William Durand",
        "email": "will+git@drnd.me"
    }
    ],
    "require": {
        "symfony/framework-bundle": "~2.1|~3.0"
    },
    "require-dev": {
        "symfony/finder": "~2.1|~3.0",
        "symfony/browser-kit": "~2.1|~3.0",
        "symfony/yaml": "~2.1|~3.0",
        "symfony/form": "~2.1|~3.0",
        "sensio/framework-extra-bundle": "~2.1|~3.0"
    },
    "autoload": {
        "psr-4": { "Bazinga\\Bundle\\RestExtraBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    }
}
