{
    "name": "sandbox/php-rest-api",
    "license": "proprietary",
    "type": "project",
    "description": "PHP REST API for Sandbox",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "symfony/icu": "1.0.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "doctrine/migrations": "^1.4",
        "doctrine/doctrine-migrations-bundle": "1.0.*",
        "beberlei/DoctrineExtensions": "^1.0",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/swiftmailer-bundle": "2.3.*",
        "symfony/monolog-bundle": "~2.3",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "friendsofsymfony/rest-bundle": "1.4.*",
        "nelmio/api-doc-bundle": "~2.4",
        "nelmio/cors-bundle": "~1.3.2",
        "friendsofsymfony/http-cache-bundle": "~1.0",
        "willdurand/hateoas-bundle": "1.0.*@dev",
        "hautelook/templated-uri-bundle": "~2.0",
        "willdurand/rest-extra-bundle": "~1.0",
        "php-jsonpatch/php-jsonpatch": "dev-master",
        "pingplusplus/pingpp-php": "dev-master",
        "knplabs/knp-paginator-bundle": "^2.4",
        "friendsofsymfony/elastica-bundle": "^3.1",
        "liuggio/excelbundle": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "@stable",
        "fabpot/php-cs-fixer": "@stable",
        "php-jsonpointer/php-jsonpointer": "1.1.*@dev",
        "php-jsonpatch/php-jsonpatch": "@dev"
    },
    "conflict": {
        "symfony/routing": "2.4.1"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "bash contrib/setup.sh"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    },
    "repositories": [
        {"type": "composer", "url": "https://packagist.phpcomposer.com"},
        {"packagist": false}
    ]
}
